{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi":  [2, "never"],
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-no-target-blank": "off"
    }
}
